{
    "name": "query-object-without-2-services",
    "description": "Query an object without services",
    "long-description": "A client queries an object without its 2 services.",
    "message-types": [
        "query-object",
        "query-object-reply"
    ],
    "steps": [
        {
            "type": "connect"
        },
        {
            "type": "create-object",
            "uuid": "00000000-0000-0000-0000-000000000001",
            "cookie": "set:object"
        },
        {
            "type": "create-service",
            "object-cookie": "get:object",
            "service-uuid": "00000000-0000-0000-0000-000000000002",
            "service-cookie": "set:service1",
            "version": 0
        },
        {
            "type": "create-service",
            "object-cookie": "get:object",
            "service-uuid": "00000000-0000-0000-0000-000000000003",
            "service-cookie": "set:service2",
            "version": 0
        },
        {
            "type": "send",
            "message": "query-object",
            "serial": 0,
            "uuid": "00000000-0000-0000-0000-000000000001",
            "with-services": false
        },
        {
            "type": "receive",
            "message": "query-object-reply",
            "serial": 0,
            "result": "cookie",
            "cookie": "get:object"
        }
    ]
}
