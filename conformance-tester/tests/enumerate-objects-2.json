{
    "name": "enumerate-objects-2",
    "description": "Enumerate 2 current objects",
    "long-description": "A client creates 2 objects and then enumerates them.",
    "message-types": [
        "object-created-event",
        "subscribe-objects",
        "subscribe-objects-reply"
    ],
    "steps": [
        {
            "type": "connect"
        },
        {
            "type": "create-object",
            "uuid": "00000000-0000-0000-0000-000000000001",
            "cookie": "set:cookie1"
        },
        {
            "type": "create-object",
            "uuid": "00000000-0000-0000-0000-000000000002",
            "cookie": "set:cookie2"
        },
        {
            "type": "send",
            "message": "subscribe-objects",
            "serial": 0
        },
        {
            "type": "receive-unordered",
            "messages": [
                {
                    "message": "object-created-event",
                    "uuid": "00000000-0000-0000-0000-000000000001",
                    "cookie": "get:cookie1",
                    "serial": 0
                },
                {
                    "message": "object-created-event",
                    "uuid": "00000000-0000-0000-0000-000000000002",
                    "cookie": "get:cookie2",
                    "serial": 0
                }
            ]
        },
        {
            "type": "receive",
            "message": "subscribe-objects-reply",
            "serial": 0
        }
    ]
}
