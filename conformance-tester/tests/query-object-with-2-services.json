{
    "name": "query-object-with-2-services",
    "description": "Query an object with 2 services",
    "long-description": "A client queries an object and its 2 services.",
    "message-types": [
        "query-object",
        "query-object-reply"
    ],
    "steps": [
        {
            "type": "connect"
        },
        {
            "type": "create-object",
            "uuid": "00000000-0000-0000-0000-000000000001",
            "cookie": "set:object"
        },
        {
            "type": "create-service",
            "object-cookie": "get:object",
            "service-uuid": "00000000-0000-0000-0000-000000000002",
            "service-cookie": "set:service1",
            "version": 0
        },
        {
            "type": "create-service",
            "object-cookie": "get:object",
            "service-uuid": "00000000-0000-0000-0000-000000000003",
            "service-cookie": "set:service2",
            "version": 0
        },
        {
            "type": "send",
            "message": "query-object",
            "serial": 0,
            "uuid": "00000000-0000-0000-0000-000000000001",
            "with-services": true
        },
        {
            "type": "receive",
            "message": "query-object-reply",
            "serial": 0,
            "result": "cookie",
            "cookie": "get:object"
        },
        {
            "type": "receive-unordered",
            "messages": [
                {
                    "message": "query-object-reply",
                    "serial": 0,
                    "result": "service",
                    "uuid": "00000000-0000-0000-0000-000000000002",
                    "cookie": "get:service1"
                },
                {
                    "message": "query-object-reply",
                    "serial": 0,
                    "result": "service",
                    "uuid": "00000000-0000-0000-0000-000000000003",
                    "cookie": "get:service2"
                }
            ]
        },
        {
            "type": "receive",
            "message": "query-object-reply",
            "serial": 0,
            "result": "done"
        }
    ]
}
