{
    "name": "enumerate-and-subscribe-objects",
    "description": "Enumerate 1 object, then destroy it",
    "long-description": "A client creates an object and enumerates it. The object is then destroyed. The broker must keep the subscription active and notify about the object destruction.",
    "message-types": [
        "object-created-event",
        "object-destroyed-event",
        "subscribe-objects",
        "subscribe-objects-reply"
    ],
    "steps": [
        {
            "type": "connect"
        },
        {
            "type": "create-object",
            "uuid": "00000000-0000-0000-0000-000000000001",
            "cookie": "set:cookie"
        },
        {
            "type": "send",
            "message": "subscribe-objects",
            "serial": 0
        },
        {
            "type": "receive",
            "message": "object-created-event",
            "uuid": "00000000-0000-0000-0000-000000000001",
            "cookie": "get:cookie",
            "serial": 0
        },
        {
            "type": "receive",
            "message": "subscribe-objects-reply",
            "serial": 0
        },
        {
            "type": "destroy-object",
            "cookie": "get:cookie"
        },
        {
            "type": "receive",
            "message": "object-destroyed-event",
            "uuid": "00000000-0000-0000-0000-000000000001",
            "cookie": "get:cookie"
        }
    ]
}
