{
    "name": "subscribe-objects-destroy",
    "description": "Subscribe to object events and destroy 1 object",
    "long-description": "A client creates an object, subscribes to object events and then destroys the object again.",
    "message-types": [
        "object-destroyed-event",
        "subscribe-objects",
        "subscribe-objects-reply"
    ],
    "steps": [
        {
            "type": "connect"
        },
        {
            "type": "create-object",
            "uuid": "00000000-0000-0000-0000-000000000001",
            "cookie": "set:cookie"
        },
        {
            "type": "send",
            "message": "subscribe-objects"
        },
        {
            "type": "destroy-object",
            "cookie": "get:cookie"
        },
        {
            "type": "receive",
            "message": "object-destroyed-event",
            "uuid": "00000000-0000-0000-0000-000000000001",
            "cookie": "get:cookie"
        }
    ]
}
