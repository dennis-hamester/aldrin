{
    "name": "enumerate-services-2",
    "description": "Enumerate 2 current services",
    "long-description": "A client creates 2 services and then enumerates them.",
    "message-types": [
        "service-created-event",
        "subscribe-services",
        "subscribe-services-reply"
    ],
    "steps": [
        {
            "type": "connect"
        },
        {
            "type": "create-object",
            "uuid": "00000000-0000-0000-0000-000000000001",
            "cookie": "set:object"
        },
        {
            "type": "create-service",
            "object-cookie": "get:object",
            "service-uuid": "00000000-0000-0000-0000-000000000002",
            "service-cookie": "set:service1",
            "version": 0
        },
        {
            "type": "create-service",
            "object-cookie": "get:object",
            "service-uuid": "00000000-0000-0000-0000-000000000003",
            "service-cookie": "set:service2",
            "version": 0
        },
        {
            "type": "send",
            "message": "subscribe-services",
            "serial": 0
        },
        {
            "type": "receive-unordered",
            "messages": [
                {
                    "message": "service-created-event",
                    "object-uuid": "00000000-0000-0000-0000-000000000001",
                    "object-cookie": "get:object",
                    "service-uuid": "00000000-0000-0000-0000-000000000002",
                    "service-cookie": "get:service1",
                    "serial": 0
                },
                {
                    "message": "service-created-event",
                    "object-uuid": "00000000-0000-0000-0000-000000000001",
                    "object-cookie": "get:object",
                    "service-uuid": "00000000-0000-0000-0000-000000000003",
                    "service-cookie": "get:service2",
                    "serial": 0
                }
            ]
        },
        {
            "type": "receive",
            "message": "subscribe-services-reply",
            "serial": 0
        }
    ]
}
