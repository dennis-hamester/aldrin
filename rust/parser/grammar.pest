WHITESPACE = _{ WHITE_SPACE }
COMMENT = _{ "//" ~ (!NEWLINE ~ ANY)* ~ NEWLINE }
ws = _{ WHITESPACE | COMMENT }
term = @{ ";" }

kw_import = @{ "import" ~ ws }

schema_name = @{ ASCII_ALPHA ~ (ASCII_ALPHANUMERIC | "_" | "-")* }

file = _{
    SOI
    ~ import_stmt*
    ~ EOI
}

import_stmt = { kw_import ~ schema_name ~ term }
